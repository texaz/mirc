                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 7
                  /writeini -n C:\Server\mIRC\numberhistoryunlock7.txt ID: $2 7).
                  /writeini -n C:\Server\mIRC\numberhistoryall15.txt ID: $2 15).
                  /msg $me !add-unbanned 7 $2 IP $2 - Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: BlackList %ming Lama: PERMANENT %ming Keterangan: Bad History (7/7) Case(s) %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) (Oleh Robot: $me  $+ )
                  /writeini -n C:\Server\mIRC\historyUBlist8.txt ID: $2 . Auto BlackList by Robot: $me $+ 
                  /writeini -n C:\Server\mIRC\numberhistoryall16.txt ID: $2 16).
                  /writeini -n C:\Server\mIRC\numberhistoryunlock8.txt ID: $2 .
                /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: IP Address Berstatus PERMANENTLY Banned tidak akan diunban! }
              }
            }
          }
        }
      }
    }
  }
}
################################
# PROMOTE AND DEMOTE USER LIST #
################################
on 1:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 2:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 3:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 4:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 5:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 6:Text:*!promote*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 7:Text:*!promote*:?: {
  ($2 == $null) {
    /msg $nick Command Promote:
    /msg $nick %server_crozz
    /msg $nick Untuk promote user ke VOP, type !promote-vop <nickname>
    /msg $nick %server_crozz
  }
}
on 7:Text:*!promote-vop*:?: {
  ($2 == $null) {
    /msg $nick Type: !promote-vop <nickname>
  }
  Else {
    /msg $nick Nickname: $2 Promoted To Rank VOP %server_name
    /raw vop $2
    /msg %server_bot1 !add $2 2
    /msg %server_bot2 !addadmin $2
    /msg $me !whois-add VOP $2
    /msg $me !cmd-add $2 2 VOP
    /notify $2
  }
}
on 7:Text:*!demote*:?: {
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == $null ) {
    /msg $nick Maaf, ID : $2 Tidak Terdaftar Sebagai Crew %server_name
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == demoted ) {
    /msg $nick Maaf, ID : $2 Sudah Di Demote Dari List Crew %server_name
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == HA ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == Admin ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == AOP ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == OPS ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == OPC ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == TE ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 $readini C:\Server\mIRC\Staff.txt ID : $2 == CAT ) {
    /msg $nick Maaf, Anda Tidak Punya Akses Untuk Menggunakan Command Ini!
    /halt
  }
  If ($2 == $null) {
    /msg $nick Silahkan Berikan ID Yang Akan Di Demote Dari Crew List %server_name
  }
  Else {
    /msg $nick ID : $2 Telah Di Demote Dari List Crew %server_name
    /raw ann ID : $2 - Telah Di Demote Dari List Crew %server_name
    /raw deop $2
    /raw devoice $2
    /msg %server_bot1 !del $2 1
    /msg %server_bot2 !deladmin $2
    /msg $me !whois-del VOP $2
    /msg $me !cmd-add $2 1
  }
}
on 8:Text:*!promote*:?: {
  ($2 == $null) {
    /msg $nick Command Promote:
    /msg $nick %server_crozz
    /msg $nick Untuk promote user ke VOP, type !promote-vop <nickname>
    /msg $nick Untuk promote user ke GAT, type !promote-gat <nickname>
    /msg $nick Untuk promote user ke TE, type !promote-te <nickname>
    /msg $nick Untuk promote user ke OPC, type !promote-opc <nickname>
    /msg $nick %server_crozz
  }
}
on 7:Text:*!promote-gat*:?: {
  ($2 == $null) {
    /msg $nick Type: !promote-gat <nickname>
  }
  Else {
    /msg $nick Nickname: $2 Promoted To Rank GAT %server_name
    /raw vop $2
    /msg %server_bot1 !add $2 2
    /msg %server_bot2 !addadmin $2
    /msg $me !whois-add CAT $2
    /msg $me !cmd-add $2 2 CAT
    /notify $2
  }
}
on 7:Text:*!promote-gat*:?: {
  ($2 == $null) {
    /msg $nick Type: !promote-gat <nickname>
  }
  Else {
    /msg $nick Nickname: $2 Promoted To Rank GAT %server_name
    /raw vop $2
    /msg %server_bot1 !add $2 2
    /msg %server_bot2 !addadmin $2
    /msg $me !whois-add CAT $2
    /msg $me !cmd-add $2 2 CAT
    /notify $2
  }
}
on 7:Text:*!promote-te*:?: {
  ($2 == $null) {
    /msg $nick Type: !promote-te <nickname>
  }
  Else {
    /msg $nick Nickname: $2 Promoted To Rank TE %server_name
