on 10:Text:*!reload*:?: }
/timer 0 250 /msg $me !sendmsg
/timer 0 500 /msg %server_ch1 Join Channel Community Untuk Free Create Room!
/timer 0 500 /msg %server_ch2 Join Channel Community Untuk Free Create Room!
/timer 0 500 /msg %server_ch3 Join Channel Community Untuk Free Create Room!
/timer 0 500 /msg %server_ch4 Join Channel Community Untuk Free Create Room!
/timer 0 500 /msg %server_ch5 Join Channel Community Untuk Free Create Room!
/timer 0 500 /msg %server_ch6 Join Channel Community Untuk Free Create Room!
/timer 0 850 /msg %server_ch1 Official Forum %server_name : %server_forum
/timer 0 850 /msg %server_ch2 Official Forum %server_name : %server_forum
/timer 0 850 /msg %server_ch3 Official Forum %server_name : %server_forum
/timer 0 850 /msg %server_ch4 Official Forum %server_name : %server_forum
/timer 0 850 /msg %server_ch5 Official Forum %server_name : %server_forum
/timer 0 850 /msg %server_ch6 Official Forum %server_name : %server_forum
/timer 0 1000 /raw ann Ayo Sebarkan Server %server_name Dengan IP : %server_ip â€¢ Tzone : %server_tzone
/timer 1 20 /msg $me !ch-admin
/timer 1 21 /msg $me Berhasil Mengisi Kembali Konfigurasi Auto Message...
}
on 10:Text:*!sendmsg*:?: {
/msg %server_ch1 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
/msg %server_ch2 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
/msg %server_ch3 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
/msg %server_ch4 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
/msg %server_ch5 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
/msg %server_ch6 %server_name â€¢ IP : %server_ip â€¢ Tzone : %server_tzone
}
