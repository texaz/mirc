on 7:Text:*!ip-plock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 1:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 2:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 3:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 4:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 5:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 6:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 7:Text:*!ip-unlock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 4:TEXT:*!lock*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !lock <nickname> <lama> <case> <bukti>
    /halt
  }
  %length = $3
  if ( %length > 90 ) { /msg $nick Maaf! Jumlah Lama ban harus berbentuk angka dan tidak lebih dari 90 Hari (MAX)
  /halt }
  else {
    if ( $3 == $null ) { 
      /notice $nick Type: !lock <nickname> <lama> <case> <bukti>
      /halt
    }
    else {
      if ($3 !isnum) { 
        /notice $nick Maaf! Perintah Lama Ban harus dalam bentuk Angka.
        /halt
      } 
      else {
        if ( $4 == $null ) { 
          /notice $nick Type: !lock <nickname> <lama> <case> <bukti>
          /halt
        }
        else {
          if ( $5 == $null ) { 
            /notice $nick Type: !lock <nickname> <lama> <case> <bukti>
            /halt
          }
          else {
            if ( $readini C:\Server\mIRC\history.txt ID: $2 == 7 ) {
              /notice $nick Jumlah Ban ID $2 Telah mencapai Jumlah Maximum
              /halt
            }
            else {
              if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == Ban ) {
                /notice $nick ID: Maaf, $2 telah berstatus banned !
                /notice $nick ID: $2 %ming $readini C:\Server\mIRC\status.txt ID: $2 
                /notice $nick $readini C:\Server\mIRC\info.txt ID: $2 %ming History Banned: ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7) Case(s).
                /halt
              }
              else {
                if ( $readini C:\Server\mIRC\blacklist.txt ID: $2 == PERMANENT ) {
                  /notice $nick Maaf, Nick yang dimaksud sudah berstatus PERMANENTLY Banned !
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\history.txt ID: $2 != 7 ) {
                  /raw Ann [BANNED INFORMATION] ID $2 %ming Status : Banned %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC\option2.txt ID: $nick - Oleh :  $nick $+ )
                  1                  /raw lockacct $2

                  /raw kill $2
                  /msg database !lock $2 $nick $4 $5-
                  /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
                  /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
                  /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 Ban
                  /notice $nick ID $2 telah di ban %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /notice $nick Link Laporan: $5-
                  %n = $calc( $3 * 86400 )
                  if ( %n >= 2000000 ) {
                    %s = $calc( %n / 2 )
                    /timer 1 %s /timer 1 %s /msg $me !unlock $2 Auto Unban
                    /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: ID $2 di ban pada tanggal: $date(dd/mm/yyyy) $time((HH:nn)) dan akan di unban pada tanggal: $calcdate($date,$3) $time((HH:nn))
                    /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Banned %ming Lama: $3 Hari %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                      /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                      /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                      /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                      /msg $me !add-banned 1 $2 ID: $2 %ming Status: Banned %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC \option2.txt ID: $nick $nick $+ )
                      /halt
                    }
                    if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                      /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                      /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                      /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                      /msg $me !add-banned 1 $2 ID: $2 %ming Status: Banned %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC \option2.txt ID: $nick $nick $+ )
                      /halt
                    }
                    elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 1 ) {
                      /writeini -n C:\Server\mIRC\history.txt ID: $2 2
                      /writeini -n C:\Server\mIRC\numberhistorylock2.txt ID: $2 2).
                      /writeini -n C:\Server\mIRC\numberhistoryall3.txt ID: $2 3).
                      /msg $me !add-banned 2 $2 ID: $2 %ming Status: Banned %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC \option2.txt ID: $nick $nick $+ )
                      /halt
                    }
                    elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 2 ) {
                      /writeini -n C:\Server\mIRC\history.txt ID: $2 3
                      /writeini -n C:\Server\mIRC\numberhistorylock3.txt ID: $2 3).
                      /writeini -n C:\Server\mIRC\numberhistoryall5.txt ID: $2 5).
                      /msg $me !add-banned 3 $2 ID: $2 %ming Status: Banned %ming Lama: $3 Hari %ming Tanggal & Jam: $date(dd/mm/yyyy) $time((HH:nn)) %ming Case: $4 %ming Link Laporan: $5- $readini C:\Server\mIRC \option2.txt ID: $nick $nick $+ )
                      /halt
