########################
# GENERAL CREW COMMAND #
########################
**************************************
* Script Announcement For Battle.Net *
**************************************
on 2:Text:*!ann*:?: {
  If ($2 == $null) {
    /msg $nick Type /w $me !ann (Info)
      /msg $nick Contoh : !ann Welcome To Server %server_name
  }
  Else {
    /raw ann $2- ( From : $nick )
    	/raw msg $nick Announcement Telah Di Kirimkan!
  }
}
*******************************************
* Script Announcement Wars For Battle.Net *
*******************************************
on 2:Text:*!wars*:?: {
  If ($2 == $null) {
    /msg $nick Type /w $me !wars (Jmlah Pemain) (Hubungi) (Info)
    	/msg $nick Contoh : !wars 5 %server_owner Need Clan Wars!
  }
  Else {
    /raw ann Need Wars : $2 vs $2 %ming Info : $4- %ming Hubungi : $3 ( From : $nick )
    	/raw msg $nick Announcement Telah Di Kirimkan!
  }
}
*********************************************
* Script Announcement By One For Battle.Net *
*********************************************
on 2:Text:*!by1*:?: {
  If ($2 == $null) {
    /msg $nick Type /w $me !by1 (Hubungi) (Info)
    	/msg $nick Contoh : !by1 %server_owner PRO GPL!
  }
  Else {
    /raw ann Need By One %ming Info : $4- %ming Hubungi : $3 ( From : $nick )
    	/raw msg $nick Announcement Telah Di Kirimkan!
  }
}
***************************************
* Script Lock / Unlock For Battle.Net *
***************************************
on 1:Text:*!lock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
on 2:Text:*!lock*:?: { /msg $nick Maaf, Anda Tidak Bisa Mengakses Command Ini! }
