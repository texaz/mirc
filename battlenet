################################
# Command Unbanned User #
################################
on 4:TEXT:!unlock*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !unlock <nickname> <alasan>
    /halt
  }
  else {
    if ( $3 == $null ) { 
      /notice $nick Type: !unlock <nickname> <alasan>
      /halt
    }
    else {
      if ( $readini C:\Server\mIRC\history.txt ID: $2 == 7 ) {
        /notice $nick Jumlah Unbanned ID $2 Telah mencapai Jumlah Maximum
        /halt
      }
      else {
        if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == unBan ) {
          /notice $nick ID: Maaf, $2 telah berstatus UnBanned !
          /notice $nick ID: $2 - $readini C:\Server\mIRC\status.txt ID: $2 
          /notice $nick $readini C:\Server\mIRC\info.txt ID: $2 %ming History Banned: ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7) Case(s).
          /halt
        }
        else {
          if ( $readini C:\Server\mIRC\blacklist.txt ID: $2 == IPPERMANENT ) {
            /notice $nick Maaf, Nick yang dimaksud telah berstatus PERMANENTLY Banned! - Silahkan Hubungi %adm
            /halt
          }
          else {
            if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == $null ) {
              /notice $nick ID: Maaf, $2 tidak berstatus Banned !

              /halt
            }
            elseif ( $readini C:\Server\mIRC\history.txt ID: $2 != 7 ) {
              /msg Database !unlock $2 $nick $3-
              /raw unlockacct $2
              /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
              /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
              /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 Unban
              /notice $nick ID $2 telah di Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
              %n = $calc( 1 * 1 )
              if ( %n >= 2000000 ) {
                %s = $calc( %n / 2 )
                /timer 1 %s /timer 1 %s /msg $me Status Unbanned untuk ID $2
                /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: Masa Banned telah habis atau sudah Konfirmasi
                /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 1 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 2
                  /writeini -n C:\Server\mIRC\numberhistoryunlock2.txt ID: $2 2).
                  /writeini -n C:\Server\mIRC\numberhistoryall4.txt ID: $2 4).
                  /msg $me !add-unbanned 2 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 2 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 3
                  /writeini -n C:\Server\mIRC\numberhistoryunlock3.txt ID: $2 3).
                  /writeini -n C:\Server\mIRC\numberhistoryall6.txt ID: $2 6).
                  /msg $me !add-unbanned 3 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 3 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 4
                  /writeini -n C:\Server\mIRC\numberhistoryunlock4.txt ID: $2 4).
                  /writeini -n C:\Server\mIRC\numberhistoryall8.txt ID: $2 .
                  /msg $me !add-unbanned 4 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 4 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 5
                  /writeini -n C:\Server\mIRC\numberhistoryunlock5.txt ID: $2 5).
                  /writeini -n C:\Server\mIRC\numberhistoryall10.txt ID: $2 10).
                  /msg $me !add-unbanned 5 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 5 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 6
                  /writeini -n C:\Server\mIRC\numberhistoryunlock6.txt ID: $2 6).
                  /writeini -n C:\Server\mIRC\numberhistoryall12.txt ID: $2 12).
                  /msg $me !add-unbanned 6 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 6 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 7
                  /writeini -n C:\Server\mIRC\numberhistoryunlock7.txt ID: $2 7).
                  /writeini -n C:\Server\mIRC\numberhistoryall15.txt ID: $2 15).
                  /msg $me !add-unbanned 7 $2 ID $2 - Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: BlackList %ming Lama: PERMANENT %ming Keterangan: Bad History (7/7) Case(s) %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) (Oleh Robot: $me  $+ )
                  /writeini -n C:\Server\mIRC\historyUBlist8.txt ID: $2 . Auto BlackList by Robot: $me $+ 
                  /writeini -n C:\Server\mIRC\numberhistoryall16.txt ID: $2 16).
                  /writeini -n C:\Server\mIRC\numberhistoryunlock8.txt ID: $2 .
                  /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: ID Berstatus PERMANENTLY Banned tidak akan diunban!
                  /halt
                }
              }
              elseif ( %n < 2000000 ) {
                /timer 1 %n /msg $me Unbanned Status ID: $2
                /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
                /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 Unban
                /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: Masa Banned telah habis atau sudah Konfirmasi
                /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 1 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 2
                  /writeini -n C:\Server\mIRC\numberhistoryunlock2.txt ID: $2 2).
                  /writeini -n C:\Server\mIRC\numberhistoryall4.txt ID: $2 4).
                  /msg $me !add-unbanned 2 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 2 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 3
                  /writeini -n C:\Server\mIRC\numberhistoryunlock3.txt ID: $2 3).
                  /writeini -n C:\Server\mIRC\numberhistoryall6.txt ID: $2 6).
                  /msg $me !add-unbanned 3 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 3 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 4
                  /writeini -n C:\Server\mIRC\numberhistoryunlock4.txt ID: $2 4).
                  /writeini -n C:\Server\mIRC\numberhistoryall8.txt ID: $2 .
                  /msg $me !add-unbanned 4 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 4 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 5
                  /writeini -n C:\Server\mIRC\numberhistoryunlock5.txt ID: $2 5).
                  /writeini -n C:\Server\mIRC\numberhistoryall10.txt ID: $2 10).
                  /msg $me !add-unbanned 5 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 5 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 6
                  /writeini -n C:\Server\mIRC\numberhistoryunlock6.txt ID: $2 6).
                  /writeini -n C:\Server\mIRC\numberhistoryall12.txt ID: $2 12).
                  /msg $me !add-unbanned 6 $2 ID $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 6 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 7
                  /writeini -n C:\Server\mIRC\numberhistoryunlock7.txt ID: $2 7).
                  /writeini -n C:\Server\mIRC\numberhistoryall15.txt ID: $2 15).
                  /msg $me !add-unbanned 7 $2 ID $2 - Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: BlackList %ming Lama: PERMANENT %ming Keterangan: Bad History (7/7) Case(s) %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) (Oleh Robot: $me  $+ )
                  /writeini -n C:\Server\mIRC\historyUBlist8.txt ID: $2 . Auto BlackList by Robot: $me $+ 
                  /writeini -n C:\Server\mIRC\numberhistoryall16.txt ID: $2 16).
                  /writeini -n C:\Server\mIRC\numberhistoryunlock8.txt ID: $2 .
                /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: ID Berstatus PERMANENTLY Banned tidak akan diunban! }
              }
            }
          }
        }
      }
    }
  }
}
################################
# Command Unbanned IP #
################################
on 8:TEXT:!ip-unlock*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !ip-unlock <ip> <alasan>
    /halt
  }
  else {
    if ( $3 == $null ) { 
      /notice $nick Type: !ip-unlock <ip> <alasan>
      /halt
    }
    else {
      if ( $readini C:\Server\mIRC\history.txt ID: $2 == 7 ) {
        /notice $nick Jumlah Unbanned IP Address $2 Telah mencapai Jumlah Maximum
        /halt
      }
      else {
        if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == ipunBan ) {
          /notice $nick Maaf, IP Address $2 telah berstatus UnBanned !
          /notice $nick IP: $2 - $readini C:\Server\mIRC\status.txt ID: $2 
          /notice $nick $readini C:\Server\mIRC\info.txt ID: $2 %ming History Banned: ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7) Case(s).
          /halt
        }
        else {
          if ( $readini C:\Server\mIRC\blacklist.txt ID: $2 == IPPERMANENT ) {
            /notice $nick Maaf, IP Address yang dimaksud telah berstatus PERMANENTLY Banned! - Silahkan Hubungi Admin $readini C:\Server\mIRC\online-Admin.txt ID: Online-Admin
            /halt
          }
          else {
            if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == $null ) {
              /notice $nick ID: Maaf, IP Address $2 tidak berstatus Banned !
              /halt
            }
            elseif ( $readini C:\Server\mIRC\history.txt ID: $2 != 7 ) {
              /raw ipban del $2
              /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
              /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
              /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 IPUNBAN
              /notice $nick IP $2 telah di Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
              %n = $calc( 1 * 1 )
              if ( %n >= 2000000 ) {
                %s = $calc( %n / 2 )
                /timer 1 %s /timer 1 %s /msg $me Status Unbanned untuk ID $2
                /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: Masa Banned telah habis atau sudah Konfirmasi kpd Administrator
                /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                if ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == $null ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 1
                  /writeini -n C:\Server\mIRC\numberhistoryunlock1.txt ID: $2 1).
                  /writeini -n C:\Server\mIRC\numberhistoryall2.txt ID: $2 2).
                  /msg $me !add-unbanned 1 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 1 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 2
                  /writeini -n C:\Server\mIRC\numberhistoryunlock2.txt ID: $2 2).
                  /writeini -n C:\Server\mIRC\numberhistoryall4.txt ID: $2 4).
                  /msg $me !add-unbanned 2 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 2 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 3
                  /writeini -n C:\Server\mIRC\numberhistoryunlock3.txt ID: $2 3).
                  /writeini -n C:\Server\mIRC\numberhistoryall6.txt ID: $2 6).
                  /msg $me !add-unbanned 3 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 3 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 4
                  /writeini -n C:\Server\mIRC\numberhistoryunlock4.txt ID: $2 4).
                  /writeini -n C:\Server\mIRC\numberhistoryall8.txt ID: $2 .
                  /msg $me !add-unbanned 4 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 4 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 5
                  /writeini -n C:\Server\mIRC\numberhistoryunlock5.txt ID: $2 5).
                  /writeini -n C:\Server\mIRC\numberhistoryall10.txt ID: $2 10).
                  /msg $me !add-unbanned 5 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 5 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 6
                  /writeini -n C:\Server\mIRC\numberhistoryunlock6.txt ID: $2 6).
                  /writeini -n C:\Server\mIRC\numberhistoryall12.txt ID: $2 12).
                  /msg $me !add-unbanned 6 $2 IP $2 - Status: Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  /halt
                }
                elseif ( $readini C:\Server\mIRC\historyUB.txt ID: $2 == 6 ) {
                  /writeini -n C:\Server\mIRC\historyUB.txt ID: $2 7
                  /writeini -n C:\Server\mIRC\numberhistoryunlock7.txt ID: $2 7).
                  /writeini -n C:\Server\mIRC\numberhistoryall15.txt ID: $2 15).
                  /msg $me !add-unbanned 7 $2 IP $2 - Unbanned %ming Tanggal Unbanned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
