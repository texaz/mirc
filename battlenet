                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 5 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 6
                    /writeini -n C:\Server\mIRC\numberhistorylock6.txt ID: $2 6).
                    /writeini -n C:\Server\mIRC\numberhistoryall11.txt ID: $2 11).
                    /msg $me !add-banned 6 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 6 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 7
                    /writeini -n C:\Server\mIRC\numberhistorylock7.txt ID: $2 7).
                    /writeini -n C:\Server\mIRC\numberhistoryall13.txt ID: $2 13).
                    /msg $me !add-banned 7 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: BlackList %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: Bad History (7/7) (Oleh Robot: $me $+ )
