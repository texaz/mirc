#############################
# Command Banned IP #
#############################
on 8:TEXT:!ip-lock*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !ip-lock <ip> <lama> <alasan>
    /halt
  }
  else {
    if ( $3 == $null ) { 
      /notice $nick Type: !ip-lock <ip> <lama> <alasan>
      /halt
    }
    else {
      if ($3 !isnum) { 
        /notice $nick Maaf! Perintah Lama Banned harus dalam bentuk Angka.
        /halt
      } 
      else {
        if ( $4 == $null ) { 
          /notice $nick Type: !ip-lock <ip> <lama> <alasan> 
          /halt
        }
        else {
          if ( $readini C:\Server\mIRC\history.txt ID: $2 == 7 ) {
            /notice $nick Jumlah Banned IP $2 Telah mencapai Jumlah Maximum
            /halt
          }
          else {
            if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == IPBan ) {
              /notice $nick IP: Maaf, IP: $2 telah berstatus Banned !
              /notice $nick IP: $2 - $readini C:\Server\mIRC\status.txt ID: $2 
              /notice $nick $readini C:\Server\mIRC\info.txt ID: $2 %ming History Banned: ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7) Case(s).
              /halt
            }
            else {
              if ( $readini C:\Server\mIRC\blacklist.txt ID: $2 == IPPERMANENT ) {
                /notice $nick Maaf, IP Address yang dimaksud sudah berstatus PERMANENTLY Banned !
                /halt
              }
              elseif ( $readini C:\Server\mIRC\history.txt ID: $2 != 7 ) {
                /raw ipban add $2
                /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
                /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
                /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 IPBan
                /notice $nick IP $2 telah di banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                %n = $calc( $3 * 86400 )
                if ( %n >= 2000000 ) {
                  %s = $calc( %n / 2 )
                  /timer 1 %s /timer 1 %s /msg $me !ip-unlock $2 Auto Unbanned
                  /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: IP $2 akan di Unbanned pada Tanggal: $calcdate($date,$3) $time((HH:nn))
                  /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Banned %ming Lama: $3 Hari %ming Tanggal: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                  if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                    /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                    /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                    /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                    /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                    /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                    /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 1 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 2
                    /writeini -n C:\Server\mIRC\numberhistorylock2.txt ID: $2 2).
                    /writeini -n C:\Server\mIRC\numberhistoryall3.txt ID: $2 3).
                    /msg $me !add-banned 2 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 2 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 3
                    /writeini -n C:\Server\mIRC\numberhistorylock3.txt ID: $2 3).
                    /writeini -n C:\Server\mIRC\numberhistoryall5.txt ID: $2 5).
                    /msg $me !add-banned 3 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 3 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 4
                    /writeini -n C:\Server\mIRC\numberhistorylock4.txt ID: $2 4).
                    /writeini -n C:\Server\mIRC\numberhistoryall7.txt ID: $2 7).
                    /msg $me !add-banned 4 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 4 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 5
                    /writeini -n C:\Server\mIRC\numberhistorylock5.txt ID: $2 5).
                    /writeini -n C:\Server\mIRC\numberhistoryall9.txt ID: $2 9).
                    /msg $me !add-banned 5 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 5 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 6
                    /writeini -n C:\Server\mIRC\numberhistorylock6.txt ID: $2 6).
                    /writeini -n C:\Server\mIRC\numberhistoryall11.txt ID: $2 11).
                    /msg $me !add-banned 6 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 6 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 7
                    /writeini -n C:\Server\mIRC\numberhistorylock7.txt ID: $2 7).
                    /writeini -n C:\Server\mIRC\numberhistoryall13.txt ID: $2 13).
                    /msg $me !add-banned 7 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: BlackList %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: Bad History (7/7) (Oleh Robot: $me $+ )
                    /writeini -n C:\Server\mIRC\historylist8.txt ID: $2 . Auto BlackList by Robot: $me $+ 
                    /writeini -n C:\Server\mIRC\numberhistoryall14.txt ID: $2 14).
                    /writeini -n C:\Server\mIRC\numberhistorylock8.txt ID: $2 .
                    /raw Ann IP $2 telah diBlackList %ming Lama: Permanent %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: Bad History (7/7) (Oleh Robot: $me $+ )
                    /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: IP Address Berstatus PERMANENTLY Banned tidak akan di Unban!
                    /halt
                  }
                }
                elseif ( %n < 2000000 ) {
                  /timer 1 %n /msg $me !ip-unlock $2 Auto Unbanned
                  /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
                  /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
                  /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 IPBan
                  /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: IP $2 akan di Unbanned pada Tanggal: $calcdate($date,$3) $time((HH:nn))
                  /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick  $nick $+ )
                  if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                    /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                    /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                    /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                    /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                    /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                    /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 1 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 2
                    /writeini -n C:\Server\mIRC\numberhistorylock2.txt ID: $2 2).
                    /writeini -n C:\Server\mIRC\numberhistoryall3.txt ID: $2 3).
                    /msg $me !add-banned 2 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 2 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 3
                    /writeini -n C:\Server\mIRC\numberhistorylock3.txt ID: $2 3).
                    /writeini -n C:\Server\mIRC\numberhistoryall5.txt ID: $2 5).
                    /msg $me !add-banned 3 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
