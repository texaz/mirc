                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 3 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 4
                    /writeini -n C:\Server\mIRC\numberhistorylock4.txt ID: $2 4).
                    /writeini -n C:\Server\mIRC\numberhistoryall7.txt ID: $2 7).
                    /msg $me !add-banned 4 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
                  }
                  elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 4 ) {
                    /writeini -n C:\Server\mIRC\history.txt ID: $2 5
                    /writeini -n C:\Server\mIRC\numberhistorylock5.txt ID: $2 5).
                    /writeini -n C:\Server\mIRC\numberhistoryall9.txt ID: $2 9).
                    /msg $me !add-banned 5 $2 IP $2 - Status: Banned %ming Lama: $3 Hari %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $4- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
                    /halt
