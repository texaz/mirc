    /msg %server_ch4 %cat $+ .
    /msg %server_ch5 %cat $+ .
    /msg %server_ch6 %cat $+ .
    /amsg %cat $+ .
  }
  else /amsg Saat Ini Tidak Ada CheAT analyzer Online.
}
on 1:Text:*!ch-VOP*:?: {
  /msg $me !whois VOP
  /timer 1 1 /msg $me !vop
  /timer 1 450 /msg $me !ch-HA
}
on 10:Text:*!vop*:?: {
  /msg $me !whois VOP
  /set %vop VOP Online :
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i).ison) {
      if ($level($notify(%i)) == 3 ) {
        /inc %j 1
        if (%a == 0) %vop = %vop $notify(%i)
        else %vop = %vop $+ , $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) {
    /msg %server_ch2 %vop $+ .
    /msg %server_ch3 %vop $+ .
    /msg %server_ch4 %vop $+ .
    /msg %server_ch5 %vop $+ .
    /msg %server_ch6 %vop $+ .
    /amsg %vop $+ .
  }
  else /amsg Saat Ini Tidak Ada Voice OPerator Online.
}

