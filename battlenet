################
# WHISPER MODE #
################
on 1:Text:*!online*:?: {
  /msg $me !whois HA
  /msg $me !whois Admin
  /msg $me !whois AOP
  /msg $me !whois OPS
  /msg $me !whois OPC
  /msg $me !whois TE
  /msg $me !whois CAT
  /msg $me !whois VOP
  /timer 1 1 /msg $me !ol-ha $nick
  /timer 1 3 /msg $me !ol-adm $nick
  /timer 1 5 /msg $me !ol-aop $nick
  /timer 1 7 /msg $me !ol-ops $nick
  /timer 1 9 /msg $me !ol-opc $nick
  /timer 1 11 /msg $me !ol-te $nick
  /timer 1 13 /msg $me !ol-cat $nick
  /timer 1 15 /msg $me !ol-vop $nick
}
on 10:Text:*!ol-ha*:?: {
  /msg $me !whois HA
  /set %haon HA Online Saat Ini :
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i).ison) {
      if ($level($notify(%i)) == 9 ) {
        /inc %j 1
        if (%a == 0) %haon = %haon $notify(%i)
        else %haon = %haon $+ , $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %haon $+ .
  else /msg $2 Saat Ini Tidak Ada Head Admin Yang Online.
}
on 10:Text:*!ol-adm*:?: {
  /msg $me !whois Admin
  /set %admon HA Online Saat Ini :
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i).ison) {
      if ($level($notify(%i)) == 8 ) {
