#################
# CONFIGURATION #
#################
on 1:Connect: {
  /set %server_owner
  /set %server_dev
  /set %server_password
  /set %server_name
  /set %server_ip
  /set %server_tzone
  /set %server_crozz -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
  /set %server_symbol â€¢
  /set %server_forum
  /set %server_ch1
  /set %server_ch2
  /set %server_ch3
  /set %server_ch4
  /set %server_ch5
  /set %server_ch6
  /set %server_bot1
  /set %server_bot2
  /set %ming -
  /auser 10 %server_owner
  /notify %server_owner
  /notify %server_dev
  /auser 10 %server_dev
  /auser 99 %me
  /timer 1 1 /msg NICKSERV identify %server_password
  /timer 1 2 /join %server_ch1
  /timer 1 10 /msg $me !reload
  /timer 1 20 /msg %me !notify-on
  /timer 1 21 /msg %me !reload
  /writeini -n C:\Server\mIRC\option.txt ID: Line.Gate -
}
on 1:Disconnect: {
  /notify off
}
#####################
# END CONFIGURATION #
#####################

#######################
# GENERAL USE COMMAND #
#######################
on 1:Join:#: {
  /msg $nick Welcome To %server_name Battle.Net
  /msg $nick IP: %server_ip %ming Tzone: %server_tzone
  /msg $nick Untuk Melihat Command Anda, Type /w $me !help
}
on 1:Text:*!help*:?: {
  ($2 == $null) {
    /msg $nick %server_symbol Command User %server_name %server_symbol
    /msg $nick %server_crozz
    /msg $nick Untuk melihat list crew, type !crew
    /msg $nick Untuk melihat forum server, type !forum
    /msg $nick Untuk melihat status userid, type !status
    /msg $nick Untuk melihat list online crew, type !online
    /msg $nick Untuk Keluar Dari %server_name : -relog
    /msg $nick %server_crozz
  }
}
on 1:Text:*!crew*:?: {
  ($2 == $null) {
    /msg $nick *** List Crew %server_name ***
    /msg $me !list-ha $nick
    /msg $me !list-adm $nick
    /msg $me !list-aop $nick
    /msg $me !list-ops $nick
    /msg $me !list-opc $nick
    /msg $me !list-te $nick
    /msg $me !list-cat $nick
    /msg $me !list-vop $nick
  }
}     
on 1:Text:*!forum*:?: {
  ($2 == $null) {
    /msg $nick Official Forum:
    /msg $nick %server_forum
  }
}
on 1:Text:*!donasi*:?: {
  ($2 == $null) {
    /msg $nick Maaf, Untuk Saat Ini Belum Ada Donasi Yang Tersedia!
    /msg $nick Silahkan Hubungi Owner: %server_owner
  }
}
on 1:Text:*-relog:#: {
  /amsg ID : $nick Relog Dari %server_name
  /raw kill $nick
}
on 1:TEXT:!status*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !status <nickname>
    /halt
  } 
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == ipunban ) {
    /notice $nick Maaf, data yg diberikan termasuk data IP Address, silahkan gunakan command untuk melihat Status IP Address.
    /halt
  }
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == ipban ) {
    /notice $nick Maaf, data yg diberikan termasuk data IP Address, silahkan gunakan command untuk melihat Status IP Address.
    /halt
  }
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == ipPERMANENT ) {
    /notice $nick Maaf, data yg diberikan termasuk data IP Address, silahkan gunakan command untuk melihat Status IP Address.
    /halt
  }
  if ( $readini C:\Server\mIRC\status.txt ID: $2 == $null ) {
    /notice $nick Nickname: $2 Belum Pernah Di Banned!
    /halt
  }
  elseif ( $2 == $null ) { set %15 $readini C:\Server\mIRC\status.txt ID: $2 }
  if ( %15 == $null ) {
    /notice $nick ID: $2 %ming $readini C:\Server\mIRC\status.txt ID: $2 ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7)
    /notice $nick $readini C:\Server\mIRC\info.txt ID: $2
  }
}
on 1:TEXT:!cek-ip*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !cek-ip <ipaddress>
    /halt
  } 
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == unban ) {
    /notice $nick Maaf, data yg diberikan termasuk data ID, silahkan gunakan command untuk melihat Status ID.
    /halt
  }
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == ban ) {
    /notice $nick Maaf, data yg diberikan termasuk data ID, silahkan gunakan command untuk melihat Status ID.
    /halt
  }
  if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == PERMANENT ) {
    /notice $nick Maaf, data yg diberikan termasuk data ID, silahkan gunakan command untuk melihat Status ID.
    /halt
  }
  if ( $readini C:\Server\mIRC\status.txt ID: $2 == $null ) {
    /notice $nick IP $2 Belum Pernah Di Banned!
    /halt
  }
  elseif ( $2 == $null ) { set %15 $readini C:\Server\mIRC\status.txt ID: $2 }
  if ( %15 == $null ) {
    /notice $nick IP: $2 %ming $readini C:\Server\mIRC\status.txt ID: $2 
    /notice $nick $readini C:\Server\mIRC\info.txt ID: $2 %ming History Banned: ( $+ $readini C:\Server\mIRC\History.txt ID: $2 $+ / $+ 7) Case(s).
  }
}
####################
# LIST CREW SERVER #
####################
on 10:text:*!list-owner*:?: {
  /msg $2 Owner %server_name : %server_owner
}
on 10:text:*!list-dev*:?: {
  /msg $2 Dev %server_name : %server_dev
}
on 10:text:*!list-ha*:?: {
  /set %hadm1 HeadAdmin:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 9 ) {
        /inc %j 1
        if (%a == 0) %hadm1 = %hadm1 $notify(%i)
        else %hadm1 = %hadm1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %hadm1 
  else /msg $2 NULL Head Admin.
}
on 10:text:*!list-adm*:?: {
  /set %adm1 Administrator:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 8 ) {
        /inc %j 1
        if (%a == 0) %adm1 = %adm1 $notify(%i)
        else %adm1 = %adm1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %adm1 
  else /msg $2 NULL Administrator.
}
on 10:text:*!list-aop*:?: {
  /set %aop1 AdminOPerator:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 7 ) {
        /inc %j 1
        if (%a == 0) %aop1 = %aop1 $notify(%i)
        else %aop1 = %aop1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %aop1 
  else /msg $2 NULL Admin OPerator.
}
on 10:text:*!list-ops*:?: {
  /set %HOP1 OPeratorServer:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 6 ) {
        /inc %j 1
        if (%a == 0) %HOP1 = %HOP1 $notify(%i)
        else %HOP1 = %HOP1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %HOP1 
  else /msg $2 NULL OPerator Server.
}
on 10:text:*!list-opc*:?: {
  /set %opc1 OPeratorChanel:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 5 ) {
        /inc %j 1
        if (%a == 0) %opc1 = %opc1 $notify(%i)
        else %opc1 = %opc1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %opc1 
  else /msg $2 NULL OPerator Channel.
}
on 10:text:*!list-TE*:?: {
  /set %M-Track1 TeamEvent:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 4 ) {
        /inc %j 1
        if (%a == 0) %M-Track1 = %M-Track1 $notify(%i)
        else %M-Track1 = %M-Track1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %M-Track1 
  else /msg $2 NULL Team Event.
}
on 10:text:*!list-cat*:?: {
  /set %cat1 GenxAnalyzer:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 3 ) {
        /inc %j 1
        if (%a == 0) %cat1 = %cat1 $notify(%i)
        else %cat1 = %cat1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %cat1 
  else /msg $2 NULL Analyzet Team.
}
on 10:text:*!list-vop*:?: {
  /set %vop1 VoiceOPerator:
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i)) {
      if ($level($notify(%i)) == 2 ) {
        /inc %j 1
        if (%a == 0) %vop1 = %vop1 $notify(%i)
        else %vop1 = %vop1 %ming $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) /msg $2 %vop1 
  else /msg $2 NULL Voice OPerator.
}

