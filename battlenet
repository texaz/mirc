on 1:Text:*!ch-HA*:?: {
  /msg $me !whois HA
  /timer 1 1 /msg $me !ha
  /timer 1 50 /msg $me !ch-Admin
}
on 10:Text:*!ha*:?: {
  /msg $me !whois HA
  /set %ha HA Online :
  /var %i = 1
  /var %a = 0
  /var %j = 0
  while (%i <= $notify(0)) {
    if ($notify(%i).ison) {
      if ($level($notify(%i)) == 9 ) {
        /inc %j 1
        if (%a == 0) %ha = %ha $notify(%i)
        else %ha = %ha $+ , $notify(%i)
        %a = 1
      }
    }
    /inc %i 1
  }
  if (%j >= 1) {
    /msg %server_ch2 %ha $+ .
    /msg %server_ch3 %ha $+ .
    /msg %server_ch4 %ha $+ .
    /msg %server_ch5 %ha $+ .
    /msg %server_ch6 %ha $+ .
    /amsg %ha $+ .
  }
  else /amsg Saat Ini Tidak Ada Head Admin Online.
}
on 1:Text:*!ch-Admin*:?: {
  /msg $me !whois Admin
  /timer 1 1 /msg $me !admin
  /timer 1 100 /msg $me !ch-AOP
}
