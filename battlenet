on 8:TEXT:!ip-plock*:?: { 
  if ( $2 == $null ) { 
    /notice $nick Type: !ip-plock <ip> <alasan> 
    /halt
  }
  else {
    if ( $3 == $null ) { 
      /notice $nick Type: !ip-plock <ip> <alasan> 
      /halt
    }
    else {
      if ( $readini C:\Server\mIRC\history.txt ID: $2 == 7 ) {
        /notice $nick Jumlah Banned IP Address $2 Telah mencapai Jumlah Maximum
        /halt
      }
      else {
        if ( $readini C:\Server\mIRC\lockunlock.txt ID: $2 == IPban ) {
          /notice $nick Maaf, IP Address $2 sudah berstatus Banned, Unlock lah terlebih dahulu.
          /halt
        }
        else {
          if ( $readini C:\Server\mIRC\blacklist.txt ID: $2 == IPPERMANENT ) {
            /notice $nick Maaf, IP Address yang dimaksud sudah berstatus PERMANENTLY Banned !
            /halt
          }
          elseif ( $readini C:\Server\mIRC\history.txt ID: $2 != 7 ) {
            /raw ipban add $2
            /writeini -n C:\Server\mIRC\lastcommand.txt ID: $nick $1-
            /msg #Admin Date: $day $+ , $date(dd/mm/yyyy) - Time: $time((HH:nn)) %ming $readini C:\Server\mIRC\StaffJoin.txt ID: $nick $nick has been Using command: $1-
            /notice $nick $2 telah di banned PERMANENT %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick $+ )
            %n = $calc( $3 * 10 )
            if ( %n >= 2000000 ) {
              %s = $calc( %n / 2 )
              /timer 1 %s /timer 1 %s /msg $me Permanent Banned tidak akan di Unban
              /writeini -n C:\Server\mIRC\status.txt ID: $2 Status: Banned %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID:  $nick $nick $+ )
              /writeini -n C:\Server\mIRC\blacklist.txt ID: $2 IPPERMANENT
              /writeini -n C:\Server\mIRC\lockunlock.txt ID: $2 ipban
              /writeini -n C:\Server\mIRC\info.txt ID: $2 Info: IP Address Berstatus PERMANENTLY Banned tidak akan di Unban!
              if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick  $+ )
                /halt
              }
              if ( $readini C:\Server\mIRC\history.txt ID: $2 == $null ) {
                /writeini -n C:\Server\mIRC\history.txt ID: $2 1
                /writeini -n C:\Server\mIRC\numberhistorylock1.txt ID: $2 1).
                /writeini -n C:\Server\mIRC\numberhistoryall1.txt ID: $2 1).
                /msg $me !add-banned 1 $2 IP $2 - Status: Banned %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick  $+ )
                /halt
              }
              elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 1 ) {
                /writeini -n C:\Server\mIRC\history.txt ID: $2 2
                /writeini -n C:\Server\mIRC\numberhistorylock2.txt ID: $2 2).
                /writeini -n C:\Server\mIRC\numberhistoryall3.txt ID: $2 3).
                /msg $me !add-banned 2 $2 IP $2 - Status: Banned %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick  $+ )
                /halt
              }
              elseif ( $readini C:\Server\mIRC\history.txt ID: $2 == 2 ) {
                /writeini -n C:\Server\mIRC\history.txt ID: $2 3
                /writeini -n C:\Server\mIRC\numberhistorylock3.txt ID: $2 3).
                /writeini -n C:\Server\mIRC\numberhistoryall5.txt ID: $2 5).
                /msg $me !add-banned 3 $2 IP $2 - Status: Banned %ming Lama: PERMANENT %ming Tanggal Banned: $date(dd/mm/yyyy) $time((HH:nn)) %ming Keterangan: $3- $readini C:\Server\mIRC\option2.txt ID: $nick $nick  $+ )
