#############
# CMD - BOT #
#############
on 10:Text:*!cmd-freeze*:?: {
  ($2 == $null) {
    /msg $nick Maaf, Command Anda Salah!
    /msg $nick Type /w $me !cmd-freeze (Username)
    /msg $nick Contoh : !cmd-freeze %server_owner
  }
  Else {
    /msg $nick ID : $2 Telah Di Freeze Command!
    /msg $me !cmd-add $2 Freeze
  }
}
on 10:Text:*!cmd-lvl*:?: {
  ($2 == $null) {
    /msg $nick Level Command :
    /msg $nick Level 1 = User
    /msg $nick Level 2 = VOP
    /msg $nick Level 3 = CAT
    /msg $nick Level 4 = TE
    /msg $nick Level 5 = OPC
    /msg $nick Level 6 = OPS
    /msg $nick Level 7 = AOP
    /msg $nick Level 8 = Admin
    /msg $nick Level 9 = HA
    /msg $nick Level ? = ??
    /msg $nick Group Available :
    /msg $nick HA, Admin, AOP, OPS, OPC, TE, CAT, VOP.
  }
}
on 10:Text:*!cmd-add*:?: {
  alias hire {
    If ($2 == $null) {
      /msg $nick Maaf, Command Belum Lengkap!
      /msg $nick Type /w $me !cmd-add (Username) (Level) (Group)
      /msg $nick Contoh : !cmd-add %server_dev 9 HA
    }
    Else {
      /auser $3 $2
      If ($3 == 1 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 Devoice/deop/
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 Demoted
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Hapus Dari List Crew!
      }
      If ($3 == 2 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List VOP Dengan Group : $4-
      }
      If ($3 == 3 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List CAT Dengan Group : $4-
      }
      If ($3 == 4 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List TE Dengan Group : $4-
      }
      If ($3 == 5 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List OPC Dengan Group : $4-
      }
      If ($3 == 6 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List OPS Dengan Group : $4-
      }
      If ($3 == 7 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List AOP Dengan Group : $4-
      }
      If ($3 == 8 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Masukan Ke List Admin Dengan Group : $4-
      }
      If ($3 == 9 ) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 $4- $+ :
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 $4
        /writeini -n C:\Server\mIRC\Option2.txt ID : $2 ( $+ $4- $+ :
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /writeini -n C:\Server\mIRC\Option.txt ID : Pembatas |
        /msg $nick ID : $2 Telah Di Masukan Ke List HA Dengan Group : $4-
      }
    }
  }
}
on 10:Text:*!cmd-del*:?: {
  alias hire {
    If ($2 == $null) {
      /msg $nick Maaf, Command Belum Lengkap!
      /msg $nick Type /w $me !cmd-del (Username)
      /msg $nick Contoh : !cmd-del %server_dev
      /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
    }
    Else {
      /auser $3 $2
      If ($read(userlist.txt, s, $2) == $null) {
        write users.ini $3 $2
        write remote.ini $3 $2
        writeini sbot.ini status users $calc($users + 1)
      }
      If ($3 == $null) {
        /writeini -n C:\Server\mIRC\Option.txt ID : $2 Devoice/deop/
        /writeini -n C:\Server\mIRC\Staff.txt ID : $2 DeCrew
        /writeini -n C:\Server\mIRC\lastcommand.txt ID : $nick $1-
        /msg $nick ID : $2 Telah Di Hapus Dari List Crew!
      }
    }
  }
}
################
# SYSTEM WHOIS #
################
on 10:Text:*!whois-add*:?: {
  If ($2 == HA) { write C:\Server\mIRC\ListHA.txt $3-
  /msg $nick Menambahkan $3- Ke List Head Admin. }
  If ($2 == Admin) { write C:\Server\mIRC\ListAdmin.txt $3-
  /msg $nick Menambahkan $3- Ke List Administrator. }
  If ($2 == AOP) { write C:\Server\mIRC\ListAOP.txt $3-
  /msg $nick Menambahkan $3- Ke List Admin OPerator. }
  If ($2 == OPS) { write C:\Server\mIRC\ListOPS.txt $3-
  /msg $nick Menambahkan $3- Ke List OPerator Server. }
  If ($2 == OPC) { write C:\Server\mIRC\ListOPC.txt $3-
  /msg $nick Menambahkan $3- Ke List OPerator Channel. }
  If ($2 == TE) { write C:\Server\mIRC\ListTE.txt $3-
  /msg $nick Menambahkan $3- Ke List Team Event. }
  If ($2 == CAT) { write C:\Server\mIRC\ListCAT.txt $3-
  /msg $nick Menambahkan $3- Ke List CheAT analyzer. }
  If ($2 == VOP) { write C:\Server\mIRC\ListVOP.txt $3-
  /msg $nick Menambahkan $3- Ke List Voice OPerator. }
}
on 10:Text:*!whois-del*:?: {
  If ($2 == HA) { write $+(-ds,$3) C:\Server\mIRC\ListHA.txt $3-
  /msg $nick Menghapus $3- Ke List Head Admin. }
  If ($2 == Admin) { write $+(-ds,$3) C:\Server\mIRC\ListAdmin.txt $3-
  /msg $nick Menghapus $3- Ke List Administrator. }
  If ($2 == AOP) { write $+(-ds,$3) C:\Server\mIRC\ListAOP.txt $3-
  /msg $nick Menghapus $3- Ke List Admin OPerator. }
  If ($2 == OPS) { write $+(-ds,$3) C:\Server\mIRC\ListOPS.txt $3-
  /msg $nick Menghapus $3- Ke List OPerator Server. }
  If ($2 == OPC) { write $+(-ds,$3) C:\Server\mIRC\ListOPC.txt $3-
  /msg $nick Menghapus $3- Ke List OPerator Channel. }
  If ($2 == TE) { write $+(-ds,$3) C:\Server\mIRC\ListTE.txt $3-
  /msg $nick Menghapus $3- Ke List Team Event. }
  If ($2 == CAT) { write $+(-ds,$3) C:\Server\mIRC\ListCAT.txt $3-
  /msg $nick Menghapus $3- Ke List CheAT analyzer. }
  If ($2 == VOP) { write $+(-ds,$3) C:\Server\mIRC\ListVOP.txt $3-
  /msg $nick Menghapus $3- Ke List Voice OPerator. }
}
on 10:Text:*!whois*:?: {
  If ($2 == Owner) {
    var %a = $lines(C:\Server\mIRC\ListOwner.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListOwner.txt,%a)
      /whois $read(C:\Server\mIRC\ListOwner.txt,%a)
      dec %a
    }
  }
  If ($2 == Dev) {
    var %a = $lines(C:\Server\mIRC\ListDev.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListDev.txt,%a)
      /whois $read(C:\Server\mIRC\ListDev.txt,%a)
      dec %a
    }
  }
  If ($2 == HA) {
    var %a = $lines(C:\Server\mIRC\ListHA.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListHA.txt,%a)
      /whois $read(C:\Server\mIRC\ListHA.txt,%a)
      dec %a
    }
  }
  If ($2 == Admin) {
    var %a = $lines(C:\Server\mIRC\ListAdmin.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListAdmin.txt,%a)
      /whois $read(C:\Server\mIRC\ListAdmin.txt,%a)
      dec %a
    }
  }
  If ($2 == AOP) {
    var %a = $lines(C:\Server\mIRC\ListAOP.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListAOP.txt,%a)
      /whois $read(C:\Server\mIRC\ListAOP.txt,%a)
      dec %a
    }
  }
  If ($2 == OPS) {
    var %a = $lines(C:\Server\mIRC\ListOPS.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListOPS.txt,%a)
      /whois $read(C:\Server\mIRC\ListOPS.txt,%a)
      dec %a
    }
  }
  If ($2 == OPC) {
    var %a = $lines(C:\Server\mIRC\ListOPC.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListOPC.txt,%a)
      /whois $read(C:\Server\mIRC\ListOPC.txt,%a)
      dec %a
    }
  }
  If ($2 == TE) {
    var %a = $lines(C:\Server\mIRC\ListTE.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListTE.txt,%a)
      /whois $read(C:\Server\mIRC\ListTE.txt,%a)
      dec %a
    }
  }
  If ($2 == CAT) {
    var %a = $lines(C:\Server\mIRC\ListCAT.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListCAT.txt,%a)
      /whois $read(C:\Server\mIRC\ListCAT.txt,%a)
      dec %a
    }
  }
  If ($2 == VOP) {
    var %a = $lines(C:\Server\mIRC\ListVOP.txt)
    while (%a) {
      /msg $nick /whois $read(C:\Server\mIRC\ListVOP.txt,%a)
      /whois $read(C:\Server\mIRC\ListVOP.txt,%a)
      dec %a
    }
  }
}
